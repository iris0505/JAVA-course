public class A {
    public final int a; // Объявление final переменной

    // Конструктор, инициализирующий переменную a
    public A(int value) {
        this.a = value; // Инициализация переменной a в конструкторе
    }

    // Блок инициализации (не используется для a, так как она уже инициализирована в конструкторе)
    {
        // a = 20; // Это вызовет ошибку, так как a уже инициализирован в конструкторе
    }

    // Пример инициализации при объявлении (закомментировано, так как это вызовет ошибку)
    // public final int a = 10; // Это вызовет ошибку, так как a уже инициализирован в конструкторе

    public static void main(String[] args) {
        A obj1 = new A(5);
        System.out.println("Значение a: " + obj1.a); // Вывод: Значение a: 5

        A obj2 = new A(10);
        System.out.println("Значение a: " + obj2.a); // Вывод: Значение a: 10
    }
}
